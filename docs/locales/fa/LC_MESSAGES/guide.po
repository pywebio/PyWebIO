"\n"
"put_table([\n"
"    ['Idx', 'Actions'],\n"
"    [1, put_buttons(['edit', 'delete'], onclick=partial(edit_row, "
"row=1))],\n"
"    [2, put_buttons(['edit', 'delete'], onclick=partial(edit_row, "
"row=2))],\n"
"    [3, put_buttons(['edit', 'delete'], onclick=partial(edit_row, "
"row=3))],\n"
"])"
msgstr ""

